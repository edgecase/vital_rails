<html>
  <body>
    <h1>Songs</h1>
    <table border=1>
      <tr>
        <th>Name</th>
        <th>Duration</th>
      </tr>
      <% @songs.each do |song| %>
        <tr>
          <td><%= song.name %></td>
          <td><%= song.duration %></td>
          <td><%= link_to "show", song_path(song.id) %>
        </tr>
      <% end %>
    </table>
  </body>
</html>